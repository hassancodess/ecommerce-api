        "start": "node dist/server.js",
        "dev": "dotenvx run -f .env.development -- tsx watch src/server",
        "prod": "dotenvx run -f .env.production -- tsx watch src/server",
        "build": "rimraf dist && tsc --noEmit false && tsc-alias",
