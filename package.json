        "globals": "^15.9.0",
        "husky": "^9.1.5",
        "prettier": "^3.3.3",
        "prisma": "^5.18.0",
        "rimraf": "^6.0.1",
        "swagger-autogen": "^2.23.7",
        "tsc-alias": "^1.8.10",
        "tsx": "^4.18.0",
        "typescript": "^5.5.4",
        "typescript-eslint": "^8.2.0"
